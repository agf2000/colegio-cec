                            <div class="pad_left1">
                                <div class="wrapper"> <strong>Matrícula:</strong>
                                    <div class="bg">
                                        <input id="inputStudentCode" type="text" class="input">
                                    </div>
                                </div>
                                <div class="wrapper"> <strong>Turma:</strong>
                                    <div class="bg">
                                        <input id="inputStudentGroup" type="text" class="input">
                                    </div>
                                </div>
                                <div class="wrapper"> <strong>Turno:</strong>
                                    <div class="bg">
                                        <input id="inputStudentShift" type="text" class="input">
                                    </div>
                                </div>
                                <div class="wrapper"> <strong>CEP:</strong>
                                    <div>
                                        <input id="inputStudentZipCode" type="text">
                                    </div>
                                </div>
                                <div class="wrapper"> <strong>Telefone Residêncial:</strong>
                                    <div>
                                        <input id="inputStudentPhone" type="text">
                                    </div>
                                </div>
                                <div class="wrapper"> <strong>Telefone Celular:</strong>
                                    <div>
                                        <input id="inputStudentCelPhone" type="text">
                                    </div>
                                </div>
                                <div class="wrapper"> <strong>Data de Nascimento:</strong>
                                    <div>
                                        <input id="kdpStudentBDate" type="date">
                                    </div>
                                </div>
                                <div class="wrapper"> <strong>Naturalidade:</strong>
                                    <div class="bg">
                                        <input id="inputBirthTown" type="text" class="input">

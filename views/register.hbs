<div class="body2">
    <div class="main">
        <!-- content -->
        <section id="content">
            <div class="box1">
                <div class="wrapper">
                    <h2>Requerimento de Matrícula</h2>
                    <form id="ContactForm" action="#">
                        <article class="col1">
                            <div class="pad_left1">
                                <div>
                                    <h3>Dados do Aluno</h3>

                                    <div class="wrapper">
                                        <input name="chkBoxStudent" type="radio" value="veteran"> Veterano
                                        <input name="chkBoxStudent" type="radio" checked value="new"> Novato
                                    </div>

                                    <div class="wrapper"> <strong>Aluno(a):</strong>
                                        <div class="bg">
                                            <input id="inputStudentName" type="text" class="input" placeholder="Nome completo do aluno(a)">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Estado:</strong>
                                        <div>
                                            <select id="sel2StudentStates" class="input">
                                                <option></option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Cidade:</strong>
                                        <div>
                                            <select id="sel2StudentCities" class="input" disabled>
                                                <option></option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Endereço:</strong>
                                        <div class="bg">
                                            <input id="inputStudentAddress" type="text" class="input" placeholder="Endereço completo do aluno">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Bairro:</strong>
                                        <div class="bg">
                                            <input id="inputStudentDistrict" type="text" class="input">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Nome do Cartório:</strong>
                                        <div class="bg">
                                            <input id="inputBirthRegisterPlaceName" type="text" class="input">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Certidão de Nascimento Termo:</strong>
                                        <div class="bg">
                                            <input id="inputBirthRegisterTerm" type="text" class="input">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Ensino:</strong>
                                        <div class="bg">
                                            <input id="inputCurriculum" type="text" class="input">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Email:</strong>
                                        <div class="bg">
                                            <input id="inputStudentEmail" type="email" class="input">
                                        </div>
                                    </div>
                                    <h3>Dados do Pai</h3>
                                    <input id="chkBoxSameAddress1" type="checkbox"><label style="font-size: 14px;">Mesmo
                                        endereço do aluno</label>
                                    <div class="wrapper"> <strong>Nome do Pai:</strong>
                                        <div class="bg">
                                            <input id="inputFatherName" type="text" class="input" placeholder="Nome completo do pai">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Profissão:</strong>
                                        <div class="bg">
                                            <input id="inputFatherProfission" type="text" class="input">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Estado:</strong>
                                        <div>
                                            <select id="sel2FatherStates" class="input">
                                                <option></option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Cidade:</strong>
                                        <div>
                                            <select id="sel2FatherCities" class="input" disabled>
                                                <option></option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Endereço:</strong>
                                        <div class="bg">
                                            <input id="inputFatherAddress" type="text" class="input" placeholder="Endereço completo do pai">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Bairro:</strong>
                                        <div class="bg">
                                            <input id="inputFatherDistrict" type="text" class="input">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Email:</strong>
                                        <div class="bg">
                                            <input id="inputFatherEmail" type="email" class="input">
                                        </div>
                                    </div>
                                    <h3>Dados da Mãe</h3>
                                    <input id="chkBoxSameAddress2" type="checkbox"><label style="font-size: 14px;">Mesmo
                                        endereço do pai</label>
                                    <div class="wrapper"> <strong>Nome da Mãe:</strong>
                                        <div class="bg">
                                            <input id="inputMotherName" type="text" class="input" placeholder="Nome completo da Mãe">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Profissão:</strong>
                                        <div class="bg">
                                            <input id="inputMotherProfission" type="text" class="input">
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Estado:</strong>
                                        <div>
                                            <select id="sel2MotherStates" class="input">
                                                <option></option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="wrapper"> <strong>Cidade:</strong>
                                        <div>
                                            <select id="sel2MotherCities" class="input" disabled>
                                                <option></option>
                                            </select>
                                        </div>
                                    </div>
